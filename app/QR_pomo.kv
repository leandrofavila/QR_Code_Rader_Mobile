MDBoxLayout:
    orientation: 'vertical'
    MDTopAppBar:
        title: 'QR'
        left_action_items: [['menu']]
        right_action_items: [['theme-light-dark']]

    MDFloatLayout:
        MDIconButton:
            icon: 'qrcode-scan'
            icon_size: '75sp'
            pos_hint: {'center_x': .5, 'center_y': .8}
        MDLabel:
            text: '25:00'
            font_style: 'H1'
            halign: 'center'

        MDRaisedButton:
            text: 'iniciar!'
            pos_hint: {'center_x': .5, 'center_y': .2}
            size_hint_x: .8
            